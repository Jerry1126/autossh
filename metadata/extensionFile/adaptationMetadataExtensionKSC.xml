<?xml version="1.0" encoding="UTF-8"?>

	<!--
		The configuration file is for OnePM TA. KOAOES setting has been
		cleaned up. Adaptation Test Data can be modified or some MID/Counter
		ignored by this configuration file.
	-->
<AdaptationExtension id="NOKKTT" release="S16.1"
	owner="nokktt" systemcomponent="umaktt" dbSchemaPrefix="NOKKTT"
	rootObject="KSC" omesFileVersion="2.1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<Mapping>
		<!--
			Unit modification in Adaptation scope is supported. The original unit
			string in RISE/AD may be different in OnePM Metadata. <Map
			type="unit" to="sec"> <From>s</From> </Map>
		-->

	</Mapping>

